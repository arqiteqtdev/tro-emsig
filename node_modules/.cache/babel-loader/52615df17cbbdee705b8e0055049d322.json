    <el-col :span=\"21\">\n        <el-input v-model=\"link\" />\n      </el-col>\n      <el-col :span=\"3\">\n        <span\n          class=\"social-field-action delete\"\n          @click=\"onDelete(index)\"\n        >\n          <i class=\"el-icon-delete\" />\n        </span>\n        <span class=\"social-field-action drag\">\n          <drag-icon />\n        </span>\n      </el-col>\n    </el-row>\n  </el-form-item>\n</template>\n\n<script>\nimport { mapState } from 'vuex'\nimport DragIcon from '../../assets/image/menu.svg'\n\nexport default {\n  name: 'SocialField',\n\n  components: {\n    DragIcon\n  },\n\n  props: {\n    index: {\n      type: Number,\n      default: 0\n    },\n    icon: {\n      type: String,\n      default: undefined\n    }\n  },\n\n  computed: {\n    ...mapState(['socials']),\n    model () {\n      return this.socials.installed.find(i => i.icon === this.icon)\n    },\n    link: {\n      get () {\n        return this.model.link\n      },\n      set (v) {\n        this.$store.dispatch('updateSocial', { index: this.index, data: v })\n      }\n    }\n  },\n\n  methods: {\n    onDelete () {\n      this.$store.dispatch('removeSocial', this.index)\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\">\n@import '../../assets/scss/variables';\n\n.social-field-action {\n  padding: 5px 7px;\n  cursor: pointer;\n  i {\n    color: #aaa;\n  }\n  &.delete {\n    &:hover {\n      i {\n        color: $color-danger;\n      }\n    }\n  }\n}\n.drag {\n  cursor: move;\n  svg {\n    position: relative;\n    top: 1px;\n    width: 13px;\n    fill: #aaa;\n  }\n}\n</style>\n"],"sourceRoot":"src/components/social"}]}