"<template>\n  <div class=\"social\">\n    <div class=\"socials\">\n      <social-icon\n        v-for=\"i in socials.list\"\n        :key=\"i.name\"\n        :data=\"i\"\n      />\n    </div>\n    <h3 />\n    <el-form label-position=\"top\">\n      <draggable\n        v-model=\"sort\"\n        :options=\"{handle: '.drag'}\"\n      >\n        <social-field\n          v-for=\"(i, index) in socials.installed\"\n          :key=\"i.name\"\n          :icon=\"i.icon\"\n          :index=\"index\"\n        />\n      </draggable>\n      <el-form-item />\n    </el-form>\n  </div>\n</template>\n\n<script>\nimport { mapState } from 'vuex'\nimport SocialIcon from '../components/social/SocialIcon'\nimport SocialField from '../components/social/SocialField'\nimport draggable from 'vuedraggable'\n\nexport default {\n  name: '',\n\n  components: {\n    SocialIcon,\n    SocialField,\n    draggable\n  },\n\n  data () {\n    return {\n      draggableOptions: {\n        handle: '.drag'\n      }\n    }\n  },\n\n  computed: {\n    ...mapState(['socials']),\n    sort: {\n      get () {\n        return this.socials.installed\n      },\n      set (v) {\n        this.$store.dispatch('updateSort', v)\n      }\n    }\n  },\n\n  created () {\n    this.$ga.page(this.$router)\n  },\n\n  methods: {\n    addSocial () {\n\n    },\n    isInstalled (name) {\n      return this.social.installed.findIndex(i => i.name === name) > -1\n    },\n    onDelete (index) {\n      this.$store.dispatch('removeSocial', index)\n    },\n    onDrag () {\n\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\">\n.socials {\n  display: grid;\n  grid-gap: 10px;\n  grid-template-columns: repeat(10, auto);\n  grid-auto-columns: 1fr;\n}\n</style>\n"],"sourceRoot":"src/views"}]}