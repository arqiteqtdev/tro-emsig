},\n\n  computed: {\n    ...mapState(['socials']),\n    model () {\n      return this.socials.installed.find(i => i.icon === this.icon)\n    },\n    link: {\n      get () {\n        return this.model.link\n      },\n      set (v) {\n        this.$store.dispatch('updateSocial', { index: this.index, data: v })\n      }\n    }\n  },\n\n  methods: {\n    onDelete () {\n      this.$store.dispatch('removeSocial', this.index)\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\">\n@import '../../assets/scss/variables';\n\n.social-field-action {\n  padding: 5px 7px;\n  cursor: pointer;\n  i {\n    color: #aaa;\n  }\n  &.delete {\n    &:hover {\n      i {\n        color: $color-danger;\n      }\n    }\n  }\n}\n.drag {\n  cursor: move;\n  svg {\n    position: relative;\n    top: 1px;\n    width: 13px;\n    fill: #aaa;\n  }\n}\n</style>\n"]}]}